bloom {
  Garden_Login : Flower {
    template = 'garden-login'

    initialize() {
      element.submit((e:browser.Event)=> {
        e.preventDefault()
        var data = {
          name: element.find('#name').val(),
          pass: element.find('#pass').val()
        }
        Garden.post('vineyard/login', data)
        .then((response)=> {
            Garden.goto('garden-hub')
          })
      })
    }
  }
}